{
  "name": "omega-game-engine",
  "version": "1.0.19",
  "description": "Omega game engine library.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build-js": "npm run build-js:dev && npm run build-js:prod",
    "build-js:dev": "rollup -c && api-extractor run --local --verbose",
    "build-js:prod": "cross-env NODE_ENV=production rollup -c && api-extractor run --local --verbose",
    "build-docs": "npx typedoc --entryPointStrategy Expand src",
    "serve-docs": "npx serve docs",
    "build-all": "npm run build && npm run build-min-js && npm run build-docs"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@microsoft/api-extractor": "^7.52.11",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "cross-env": "^10.0.0",
    "rollup": "^2.79.2",
    "rollup-plugin-terser": "^7.0.2",
    "typedoc": "^0.28.10"
  },
  "dependencies": {
    "gl-matrix": "^3.4.4",
    "tslib": "^2.8.1",
    "uuid": "^11.1.0"
  }
}
